{
  "name": "sveltepress",
  "version": "1.0.0",
  "scripts": {
    "dev": "node scripts/generateProdData.js && svelte-kit dev",
    "build": "node scripts/generateProdData.js && svelte-kit build",
    "preview": "svelte-kit preview",
    "postinstall": "ni svelte -D --ignore-scripts && node scripts/generateSymlinks.js",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "preinstall": "node scripts/prepareTheme.js"
  },
  "devDependencies": {
    "@antfu/ni": "0.12.0",
    "@sveltejs/adapter-netlify": "1.0.0-next.37",
    "@sveltejs/adapter-node": "1.0.0-next.60",
    "@sveltejs/adapter-static": "1.0.0-next.24",
    "@sveltejs/adapter-vercel": "1.0.0-next.36",
    "@sveltejs/kit": "1.0.0-next.216",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.3.0",
    "flexsearch": "0.7.21",
    "front-matter": "4.0.2",
    "highlight.js": "11.3.1",
    "jsesc": "3.0.2",
    "marked": "3.0.8",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "sass": "1.46.0",
    "svelte": "3.44.3",
    "svelte-preprocess": "4.10.1"
  },
  "type": "module",
  "dependencies": {
    "sanitize-html": "2.6.1"
  },
  "license": "Unlicense"
}